{
	"manifest_version": 2,
	"name": "Oral Chess",
	"version": "1.0",
	"description": "Play Chess Orally!",
	"icons": {
		"225": "icons/icon.png"
	},
	"browser_action": {
		"default_icon": {
			"225": "icons/icon.png"
		}
	},
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["contentScript.js"]
		}
	]
}