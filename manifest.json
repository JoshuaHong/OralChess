{
	"manifest_version": 2,
	"name": "Oral Chess",
	"version": "1.0",
	"description": "Play Chess Orally!",
	"icons": {
		"225": "icons/logo.png"
	},
	"browser_action": {
	    "default_icon": {
	      "225": "icons/logo.png"
	    }
	},
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
      		"js": ["index.js"]
  		}
	]
}